generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id       String   @id @default(auto()) @map("_id") @db.ObjectId
  name     String
  age      Int?
  email    String   @unique
  password String?
  city     String?
  avatar   String?
  status   Boolean? @default(false)
  pets     Pets[]
}


model Pets {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  name    String
  type    String?
  image   String?
  logo    String?
  status  Boolean? @default(false)
  owner   User?    @relation(fields: [ownerId], references: [id])
  ownerId String   @db.ObjectId
}
